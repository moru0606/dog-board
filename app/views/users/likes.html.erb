<h1>お気に入り</h1>
  <div class="card">
    <div class="card-header">
      <h3 class="card-title"><%= @user.name %></h3>
  </div>
  <div class="card-body">
    <p>お気に入り一覧</p>
     
      <% @like_posts.each do |post| %>
        <hr>
        <p><span>お気に入り投稿内容: </span><%= post.user.name %> : <%=link_to post.content, post_path(post.id) %></p>
      <% end %>
  </div>
  </div>
    <%= link_to '一覧戻る', posts_path, class: 'btn btn-outline-secondary' %>